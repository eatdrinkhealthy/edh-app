{
  "name": "edh-app",
  "private": true,
  "scripts": {
    "lint": "eslint . --ext .js,.jsx --ignore-pattern 'packages/**'",
    "start": "meteor run --settings settings.json",
    "test": "npm run unit-test && npm run integration-test-phantomjs",
    "unit-test": "jest -c .jest-config.json",
    "unit-test-w": "npm run unit-test -- --watchAll",
    "integration-test": "meteor test --driver-package practicalmeteor:mocha",
    "integration-test-w": "TEST_WATCH=1 meteor test --driver-package practicalmeteor:mocha",
    "integration-test-phantomjs": "meteor test --once --driver-package dispatch:mocha-phantomjs",
    "integration-test-phantomjs-w": "TEST_WATCH=1 meteor test --driver-package dispatch:mocha-phantomjs",
    "full-integration-test": "meteor test --full-app --driver-package practicalmeteor:mocha",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "dependencies": {
    "babel-runtime": "6.18.0",
    "google-map-react": "^0.22.3",
    "meteor-node-stubs": "~0.2.0",
    "react": "^15.4.2",
    "react-addons-shallow-compare": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-router": "^3.0.2",
    "react-toggle": "^2.2.0"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "babel-jest": "^18.0.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.18.0",
    "enzyme": "^2.7.0",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-meteor": "^4.0.1",
    "eslint-plugin-react": "^6.7.1",
    "jest": "^18.1.0",
    "react-addons-test-utils": "^15.4.2",
    "react-test-renderer": "^15.4.2"
  }
}
