{
  "name": "edh-app",
  "private": true,
  "scripts": {
    "start": "meteor run --settings settings.json",
    "lint": "eslint . --ext .js,.jsx --ignore-pattern 'packages/**'",
    "lint-c": "clear && yarn lint",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "flow:check": "flow check",
    "flow:status": "flow status",
    "flow:status-c": "clear && flow status",
    "flow:stop": "flow stop",
    "pretest": "yarn run lint && yarn run flow:check",
    "test": "yarn run unit-test && yarn run integration-test-full",
    "unit-test": "jest -c .jest-config.json",
    "integration-test-full": "TEST_BROWSER_DRIVER=nightmare meteor test --once --full-app --settings settings.json --driver-package meteortesting:mocha",
    "end-to-end-test": "chimp --jasmine --ddp=http://localhost:3000 --path=tests/end-to-end/",
    "storybook": "start-storybook -s ./public -p 9001 -c .storybook"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "bcrypt": "^1.0.2",
    "classnames": "^2.2.5",
    "google-map-react": "^0.24.0",
    "html-hint": "^0.2.4",
    "lodash": "^4.17.4",
    "meteor-node-stubs": "~0.2.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "react-s-alert": "^1.3.0",
    "react-toggle": "^4.0.1",
    "redux": "^3.7.1",
    "tachyons": "^4.7.4"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^3.2.0",
    "@storybook/addon-backgrounds": "^3.0.0",
    "@storybook/addon-knobs": "^3.2.0",
    "@storybook/react": "^3.2.0",
    "autoprefixer": "^7.1.2",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cheerio": "^1.0.0-rc.2",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-flowtype": "^2.34.1",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-meteor": "^4.0.1",
    "eslint-plugin-react": "^7.1.0",
    "flow-bin": "^0.49.1",
    "jest": "^20.0.4",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "nightmare": "^2.10.0",
    "postcss-easy-import": "^2.1.0",
    "react-test-renderer": "^15.6.1",
    "style-loader": "^0.18.2"
  },
  "postcss": {
    "plugins": {
      "postcss-easy-import": {},
      "autoprefixer": {
        "browsers": [
          "last 2 versions"
        ]
      }
    }
  }
}
