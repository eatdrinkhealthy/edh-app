{
  "name": "edh-app",
  "private": true,
  "scripts": {
    "start": "meteor run --settings settings.json",
    "lint": "eslint . --ext .js,.jsx --ignore-pattern 'packages/**'",
    "flow": "flow; test $? -eq 0 -o $? -eq 2",
    "flow:check": "flow check",
    "flow:status": "flow status",
    "flow:status-c": "clear && flow status",
    "flow:stop": "flow stop",
    "pretest": "yarn run lint && yarn run flow:check",
    "test": "yarn run unit-test",
    "unit-test": "jest -c .jest-config.json",
    "unit-test-w": "yarn run unit-test -- --watchAll",
    "integration-test": "meteor test --driver-package practicalmeteor:mocha",
    "integration-test-w": "TEST_WATCH=1 meteor test --driver-package practicalmeteor:mocha",
    "integration-test-phantomjs": "meteor test --once --port 4000 --driver-package dispatch:mocha-phantomjs",
    "integration-test-phantomjs-w": "TEST_WATCH=1 meteor test --port 4000 --driver-package dispatch:mocha-phantomjs",
    "full-integration-test": "meteor test --full-app --driver-package practicalmeteor:mocha",
    "storybook": "start-storybook -s ./public -p 9001 -c .storybook"
  },
  "dependencies": {
    "babel-runtime": "6.18.0",
    "classnames": "^2.2.5",
    "google-map-react": "^0.22.3",
    "lodash": "^4.17.4",
    "meteor-node-stubs": "~0.2.0",
    "react": "^15.4.2",
    "react-addons-shallow-compare": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "react-toggle": "^2.2.0",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.35.3",
    "@kadira/storybook-addon-knobs": "^1.7.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-register": "^6.18.0",
    "cheerio": "^0.22.0",
    "css-loader": "^0.26.1",
    "enzyme": "^2.7.0",
    "enzyme-to-json": "^1.4.5",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-meteor": "^4.0.1",
    "eslint-plugin-react": "^6.7.1",
    "flow-bin": "^0.39.0",
    "jest": "^18.1.0",
    "less": "^2.7.2",
    "less-loader": "^2.2.3",
    "react-addons-test-utils": "^15.4.2",
    "react-storybook-addon-backgrounds": "^0.0.7",
    "react-test-renderer": "^15.4.2",
    "style-loader": "^0.13.1"
  }
}
